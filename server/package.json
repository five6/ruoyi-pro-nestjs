{
  "name": "nest-ruoyi-server",
  "version": "1.0.0",
  "description": "NEST ROYI SERVER",
  "author": "FIVE6",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=prod nest build",
    "build:mock": "cross-env NODE_ENV=mock nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=dev nest start",
    "mock": "cross-env NODE_ENV=mock nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "start:mock": "cross-env NODE_ENV=mock node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.4",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.2.0",
    "@nestjs/platform-ws": "^9.1.6",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/testing": "^9.1.6",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^9.2.0",
    "@types/ws": "^8.5.3",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.1",
    "chance": "^1.1.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "crypto-random-string": "^5.0.0",
    "env-cmd": "^10.1.0",
    "express-rate-limit": "^6.6.0",
    "express-session": "^1.17.3",
    "express-xml-bodyparser": "^0.3.0",
